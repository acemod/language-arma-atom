{
  "scopeName": "source.arma.cfg",
  "name": "Arma Config",
  "fileTypes": [
    "cfg",
    "sqe",
    "sqm",
    "ext",
    "cpp",
    "hpp",
    "h",
    "inc"
  ],
  "patterns": [
    {
      "name": "string.single.sqf",
      "begin": "'",
      "end": "'"
    },
    {
      "name": "string.double.sqf",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.localized.sqf",
      "match": "\\$STR[a-zA-Z_]*"
    },
    {
      "include": "source.cpp"
    }
  ]
}
